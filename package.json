{
  "name": "nanohash",
  "description": "Generate short unique ids with a corresponding 64bits-based numeric reversible value",
  "version": "0.0.12",
  "main": "dist/index.js",
  "license": "MIT",
  "keywords": [
    "uuid",
    "random",
    "id",
    "url",
    "fauna"
  ],
  "files": [
    "dist/index.js",
    "dist/index.browser.js"
  ],
  "scripts": {
    "dev": "tsc -w",
    "gen-node": "tsc",
    "gen-browser": "tsc -p tsconfig.browser.json",
    "gen": "yarn gen-node && yarn gen-browser",
    "test": "jest",
    "test:watch": "jest --watch",
    "pub": "yarn gen && yarn publish"
  },
  "dependencies": {
    "nanoid": "^2.1.11"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.9.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
